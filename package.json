    "graphql-tag": "^2.12.6",
    "roboto-fontface": "*",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@nhost/core": "*",
    "@types/webfontloader": "^1.0.0",
